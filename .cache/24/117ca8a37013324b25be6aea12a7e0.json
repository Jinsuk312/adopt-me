eed.list({ animal }).then(data => {\n      updateBreeds(\n        Array.isArray(data.petfinder.breeds.breed)\n          ? data.petfinder.breeds.breed\n          : [data.petfinder.breeds.breed]\n      );\n    }, console.error);\n  }, [animal]);\n\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={e => {\n          e.preventDefault();\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={e => updateLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n        <button>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"e36da7da5f14915bce1bb9d6fae1dd6f","cacheData":{"env":{}}}